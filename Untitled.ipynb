   "source": [
    "number = int(input('Enter decimal number: '))\n",
    "temp = ''\n",
    "while True:\n",
    "    if number > 0:\n",
    "        temp += str(number % 2)\n",
    "        number = number // 2\n",
    "    else:\n",
    "        break\n",
    "result = ''\n",
    "for i in range(len(temp)-1, -1, -1):\n",
    "    result += temp[i]\n",
    "print('In binary format: ', result)"
   ]
